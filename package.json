{
  "name": "walkuere",
  "version": "1.0.6",
  "description": "walkuere",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "test": "node test/run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/limaofeng/hermes.git"
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "keywords": [
    "redux",
    "graphql"
  ],
  "author": "limaofeng@msn.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/limaofeng/hermes/issues"
  },
  "homepage": "https://github.com/limaofeng/hermes#readme",
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.0",
    "apollo-client": "^2.0.2",
    "apollo-fetch": "^0.6.0",
    "apollo-link": "^1.0.0",
    "apollo-link-batch-http": "^1.0.0",
    "apollo-link-error": "^1.0.0",
    "apollo-link-http": "^1.1.0",
    "apollo-link-ws": "^1.0.0",
    "apollo-logger": "^0.1.1",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "graphql-tools": "^2.7.2",
    "immutable": "^3.8.2",
    "react": "^16.0.0",
    "react-apollo": "^2.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.7",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-graphql": "^1.4.0-1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.1.0",
    "prettier": "^1.8.1",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-3"
    ],
    "plugins": [
      "transform-flow-strip-types",
      "inline-import"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true
  },
  "lint-staged": {
    "src/**/*.{js,jsx,graphql}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "jest": true,
      "jquery": true
    },
    "globals": {
      "__PUBLIC_URL__": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "jsx-a11y",
      "import",
      "prettier"
    ],
    "rules": {
      "indent": 0,
      "space-before-function-paren": 0,
      "prettier/prettier": "error",
      "global-require": "off",
      "comma-dangle": 0,
      "no-restricted-syntax": 0,
      "no-use-before-define": 0,
      "no-param-reassign": "warn",
      "object-curly-newline": "off",
      "function-paren-newline": "off",
      "react/prefer-stateless-function": "warn",
      "no-console": 1,
      "max-len": [
        2,
        120,
        4
      ],
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "consistent-return": 0,
      "no-confusing-arrow": 0,
      "no-shadow": 0,
      "no-underscore-dangle": 0,
      "no-debugger": 1,
      "no-continue": "warn",
      "no-plusplus": [
        "error",
        {
          "allowForLoopAfterthoughts": true
        }
      ],
      "semi": 2,
      "import/no-extraneous-dependencies": 0,
      "import/prefer-default-export": 0,
      "import/no-duplicates": 0,
      "jsx-a11y/label-has-for": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "jsx-a11y/click-events-have-key-events": 0,
      "react/no-multi-comp": 0,
      "react/no-array-index-key": [
        1
      ],
      "react/forbid-prop-types": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  }
}
