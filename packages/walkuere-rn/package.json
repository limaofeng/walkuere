{
  "name": "walkuere-rn",
  "version": "1.2.0",
  "description": "walkuere-react-native",
  "main": "./dist/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "build:lib": "tsc",
    "build:umd": "webpack --config webpack.config.js --output-filename index.js --mode development",
    "build:umd:min": "webpack --config webpack.config.js --output-filename index.min.js  --mode production",
    "clean": "rimraf lib dist",
    "prepublish": "npm run clean && npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/limaofeng/walkuere.git"
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "keywords": [
    "redux",
    "graphql"
  ],
  "author": "limaofeng@msn.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/limaofeng/walkuere/issues"
  },
  "homepage": "https://github.com/limaofeng/walkuere#readme",
  "peerDependencies": {
    "react": "^16.7.0",
    "react-native": "^0.64.1"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-client": "^2.4.12",
    "apollo-link-batch-http": "^1.2.6",
    "apollo-link-context": "^1.0.12",
    "apollo-link-error": "^1.1.5",
    "apollo-link-ws": "^1.0.12",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.4",
    "react": "^16.7.0",
    "react-apollo": "^2.4.0",
    "react-native": "^0.64.1",
    "react-navigation": "^3.0.9",
    "react-navigation-redux-helpers": "^4.0.1",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.0.0",
    "redux-thunk": "^2.3.0",
    "subscriptions-transport-ws": "^0.9.15",
    "walkuere-core": "^1.2.0"
  },
  "devDependencies": {
    "@types/graphql": "^14.0.5",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.120",
    "@types/react": "^17.0.34",
    "@types/react-native": "^0.57.32",
    "@types/react-navigation": "^3.0.1",
    "@types/react-redux": "^7.0.1",
    "@types/redux-logger": "^3.0.6",
    "@types/redux-persist": "^4.3.1",
    "@types/redux-saga": "^0.10.5",
    "jest": "^24.0.0",
    "node-fetch": "^2.3.0",
    "prettier": "^1.16.1",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.2.4",
    "webpack": "^4.29.0",
    "webpack-cli": "^4.7.0",
    "ws": "^7.4.5"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "setupFiles": [
      "<rootDir>/config/jest/setupTests.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/tests/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  }
}
